#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rol4nd909/Sites/Sqills-assignment/sales-dashboard/node_modules/.pnpm/vitest@3.2.4_@types+node@24.0.3_jsdom@26.1.0/node_modules/vitest/node_modules:/Users/rol4nd909/Sites/Sqills-assignment/sales-dashboard/node_modules/.pnpm/vitest@3.2.4_@types+node@24.0.3_jsdom@26.1.0/node_modules:/Users/rol4nd909/Sites/Sqills-assignment/sales-dashboard/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rol4nd909/Sites/Sqills-assignment/sales-dashboard/node_modules/.pnpm/vitest@3.2.4_@types+node@24.0.3_jsdom@26.1.0/node_modules/vitest/node_modules:/Users/rol4nd909/Sites/Sqills-assignment/sales-dashboard/node_modules/.pnpm/vitest@3.2.4_@types+node@24.0.3_jsdom@26.1.0/node_modules:/Users/rol4nd909/Sites/Sqills-assignment/sales-dashboard/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
